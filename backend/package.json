{
  "name": "ecotouch-backend",
  "version": "1.0.0",
  "description": "Backend API for Ecotouch platform",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node --transpile-only src/simple-server.ts",
    "dev:full": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "node dist/scripts/migrate.js",
    "db:seed": "node dist/scripts/seed.js"
  },
  "dependencies": {
    "@account-abstraction/sdk": "^0.6.0",
    "@types/express-rate-limit": "^5.1.3",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "ethers": "^6.7.0",
    "express": "^4.18.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "rate-limiter-flexible": "^3.0.0",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.1.14",
    "typeorm": "^0.3.17",
    "web3": "^4.1.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.3",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.2",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
